{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["react", "prettier"],
  "extends": ["plugin:prettier/recommended","plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error"
  }
}